{
    "manifest_version": 3,
    "name": "Tradino",
    "description": "Affiche tous les textes de la page en langage Dino et remplace les titres par des paroles de Dinos",
    "version": "1.0.0",
    "permissions": [],
    "host_permissions": [
    	"<all_urls>"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content-script/index.js"]
        }
      ],
    "browser_specific_settings": {
        "gecko": {
            "id": "tradino@breizhcamp.org",
            "strict_min_version": "42.0"
        }
    }
      
}