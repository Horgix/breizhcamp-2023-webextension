{
  "manifest_version": 3,
  "name": "Base template",
  "description": "Foo",
  "version": "1.0.0",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background/index.js",
    "type": "module"
  },
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script/index.js"]
      }
    ],
  "content_security_policy": {}
}